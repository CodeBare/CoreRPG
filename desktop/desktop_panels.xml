<root>
	<panel name="desktopdecalpanel" modes="all">
		<class>desktopdecal</class>
		<bounds>100,100,-100,-100</bounds>
		<disabled />
	</panel>

	<panel name="shortcutsanchor">
		<class>shortcutsanchor</class>
		<bounds>-304,0,0,0</bounds>
		<disabled />
	</panel>
	<panel name="shortcuts">
		<class>shortcuts</class>
		<anchored>
			<top />
			<left parent="shortcutsanchor" anchor="right" />
			<right />
			<bottom offset="-33" />
		</anchored>
	</panel>

	<panel name="imagebackpanel" modes="host,client">
		<anchored>
			<left offset="395" /> 
			<top />
			<right parent="shortcutsanchor" anchor="left" />
			<bottom offset="-33" />
		</anchored>
		<class>imagebackpanel</class>
		<disabled />
	</panel>

	<panel name="chat" modes="host">
		<class>chat</class>
		<bounds>5,90,390,-110</bounds>
		<dynamic />
		<locked />
	</panel>
	<panel name="chat" modes="client">
		<class>chat</class>
		<bounds>5,90,390,-110</bounds>
		<dynamic />
		<locked />
	</panel>

	<panel name="modifierstack" modes="host,client">
		<class>modifierstack</class>
		<bounds>5,-100,64,57</bounds>
		<dynamic />
		<locked />
	</panel>

	<panel name="dicetower" modes="host,client">
		<anchored height="90" width="90">
			<bottom offset="-35" />
			<right parent="shortcutsanchor" anchor="left" offset="-45" />
		</anchored>
		<class>dicetower</class>
		<dynamic />
		<locked />
	</panel>

	<panel name="imagefullpanel" modes="host,client">
		<anchored>
			<left /> 
			<top />
			<right />
			<bottom offset="-33" />
		</anchored>
		<class>imagefullpanel</class>
		<disabled />
	</panel>

	<panel name="characterlist">
		<anchored height="90" width="40">
			<left />
			<top />
		</anchored>
		<class>characterlist</class>
		<dynamic />
		<locked />
	</panel>
</root>